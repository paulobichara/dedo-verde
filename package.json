{
  "name": "with-react-native-web",
  "devDependencies": {
    "syncpack": "^13.0.3",
    "turbo": "^2.5.1"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.19",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
